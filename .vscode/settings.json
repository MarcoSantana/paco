{
  "vetur.validation.template": false,

  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.options": {
    "extensions": [".html", ".js", ".vue", ".scss"]
  },
  "eslint.validate": ["javascript", "vue"],

  "javascript.validate.enable": false,
  "javascript.format.enable": true,

  "[scss]": {
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "Confirmed",
    "Registrarse",
    "emulator",
    "prerender"
  ]
}
