{
  "vetur.validation.template": false,

  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "eslint.options": {
    "extensions": [".html", ".js", ".vue", ".scss"]
  },
  "eslint.validate": ["javascript", "vue"],

  "javascript.validate.enable": false,
  "javascript.format.enable": true,

  "[scss]": {
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "Confirmed",
    "Registrarse",
    "cancelled",
    "claims",
    "deleted",
    "derived",
    "different",
    "disabled",
    "emulator",
    "exceeded",
    "expired",
    "invalid",
    "large",
    "mismatch",
    "missing",
    "persistence",
    "prerender",
    "revoked",
    "unsupported",
    "wrong"
  ],
  "cSpell.language": ",es"
}
